<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.geometry.Insets?>

<VBox xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.universitymanagement.controller.AdminController.AdminDashboardController"
      style="-fx-padding: 20; -fx-alignment: center; -fx-spacing: 15;">

    <Label text="Admin Dashboard" style="-fx-font-size: 24px; -fx-font-weight: bold;"/>

    <Button text="Manage Users"
            onAction="#manageUsers"
            style="-fx-min-width: 200px; -fx-min-height: 40px;"/>

    <Button text="View Reports"
            onAction="#viewReports"
            style="-fx-min-width: 200px; -fx-min-height: 40px;"/>

    <Button text="Manage Subjects"
            onAction="#handleManageSubjects"
            style="-fx-min-width: 200px; -fx-min-height: 40px;"/>

</VBox>